            <button onAction="createCircle" label="Circle" id="arrangeCircle_button" imageMso="DiagramCycleInsertClassic" size="normal" getSupertip="getSupertip" getEnabled="atLeastTwoObjectsSelected"/>
            <button onAction="spreadObjects" label="Spread" id="spreadObjects_button" imageMso="HorizontalSpacingIncrease" size="normal" getSupertip="getSupertip" getEnabled="atLeastTwoObjectsSelected"/>
            <button onAction="compressObjects" label="Compress" id="compressObjects_button" imageMso="HorizontalSpacingDecrease" size="normal" getSupertip="getSupertip" getEnabled="atLeastTwoObjectsSelected"/>
          
