          <group id="alignGroup" label="Align">
            <button onAction="sameSize_onACtion" id="cmdSameSize" label="Same size" showLabel="true" imageMso="SizeToControlHeightAndWidth" size="normal" getEnabled="atLeastTwoObjectsSelected"/>
            <button onAction="centerVertical_onAction" id="cmdAlignVertical" label="Align horizontally" size="normal" imageMso="ObjectsAlignMiddleVertical" getEnabled="atLeastTwoObjectsSelected" />
            <button onAction="centerHorizontal_onAction" id="cmdAlignHorizontal" label="Align vertically" imageMso="ObjectsAlignCenterHorizontal" size="normal" getEnabled="atLeastTwoObjectsSelected" />
          </group>

          <group id="adjustGroup" label="Adjust">
            <button onAction="sameHeight_onAction" id="cmdSameHeight" label="Same height" showLabel="true" imageMso="SizeToControlHeight" size="normal" getEnabled="atLeastTwoObjectsSelected"/>
            <button onAction="sameWidth_onACtion" id="cmdSameWidth" label="Same width" showLabel="true" imageMso="SizeToControlWidth" size="normal" getEnabled="atLeastTwoObjectsSelected"/>
            <button onAction="sameRotation_onAction" id="cmdSameAngle" label="Same rotation" size="normal" imageMso="Repeat" getEnabled="atLeastTwoObjectsSelected" />
            <button onAction="swap_onAction" id="swap_button" label="Swap" imageMso="CircularReferences" size="normal" getSupertip="getSupertip" getEnabled="exactlyTwoObjectSelected"/>
